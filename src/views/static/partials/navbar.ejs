<nav class="nav navbar-dark bg-dark text-white justify-content-end">
            <% if (currentUser) { %>
                <span class="nav-item nav-link text-white"><%= `Signed in as ${currentUser.name}` %></span>
            <% } %>       
           
            <a href="/" class="nav-link nav-item text-white-50">Home</a>
            <a href="/wikis" class="nav-link nav-item text-white-50">Wikis</a>
            <% if(currentUser) { %> 
                <a href="/users/sign_out" class="nav-link nav-item text-white-50">Sign Out</a>
            <% } else { %>
                <a href="/users/sign_up" class="nav-link nav-item text-white-50">Sign Up</a>
                <a href="/users/sign_in" class="nav-link nav-item text-white-50">Sign In</a>    
            <% } %>
            <% if(currentUser && (!currentUser.isPremium() && !currentUser.isAdmin())) { %> 
                <a href="/users/<%= currentUser.id %>/premium" class="nav-link nav-item text-white-50">Get Premium</a>
            <% } %>
            <% if(currentUser && currentUser.isPremium()) { %>
                <a href="/users/<%= currentUser.id %>/downgrade" class="nav-link nav-item text-white-50">Go Standard</a>
            <% } %>
        
    
    
</nav>